# This file is being maintained by Puppet.
# DO NOT EDIT

pid file = /var/run/rsyncd.pid
uid = <%= @uid %>
gid = <%= @gid %>
use chroot = <%= @use_chroot %>
log format = %t %a %m %f %b
<% if @log_file -%>
log file = <%= @log_file %>
<% else %>
syslog facility = local3
<% end -%>
timeout = 300
address = <%= @address %>
<% if @motd_file != 'UNSET' -%>
motd file = <%= @motd_file %>
<% end -%>
<% if @reverse_lookup -%>
reverse lookup = <%= @reverse_lookup %>
<% end -%>
